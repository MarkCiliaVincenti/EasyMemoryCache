{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CacheSettings": {
    //"CacheProvider": "MemoryCache",
    // "CacheProvider": "Redis",
    "CacheProvider": "Memcached",

    // Redis
    "RedisSerialization": "Protobuf",
    "RedisConnectionString": "localhost:6379", //define CacheSettings:RedisConnectionString environment variable
    "RedisPassword": null, //define CacheSettings:RedisPassword environment variable

    // Memcached
    "Servers": [
      {
        "Address": "localhost",
        "Port": 11211
      }
    ],
    "socketPool": {
      "minPoolSize": "5",
      "maxPoolSize": "25",
      "connectionTimeout": "00:00:15",
      "receiveTimeout": "00:00:15",
      "deadTimeout": "00:00:15",
      "queueTimeout": "00:00:00.150"
    }
  },

  "AllowedHosts": "*"
}